{
  "functions": {
    "app/entry.server.tsx": {
      "maxDuration": 30
    }
  },
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "remix",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app/entry.server.tsx"
    }
  ]
} 